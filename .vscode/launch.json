    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_starter",
            "request": "launch",
            "type": "dart",
            "args": ["--target", "lib/main_dev.dart" ]
        },
        {
            "name": "flutter_starter (dev)",
            "request": "launch",
            "type": "dart",
            "args": ["--flavor", "dev", "--target", "lib/main_dev.dart" ]
        },
        {
            "name": "flutter_starter (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_starter (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}